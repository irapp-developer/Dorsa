<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >


    <ImageView
        android:id="@+id/frg_present_p_3_img_0"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_centerInParent="true"
        android:src="@drawable/present_p_shot_4_0"
        />
    <ImageView
        android:id="@+id/frg_present_p_3_img_1"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_centerInParent="true"
        android:src="@drawable/present_p_shot_4_1"
        />
  
    <ImageView
        android:id="@+id/frg_present_img_text"
        android:layout_width="match_parent"
        android:layout_height="45dp" 
        android:layout_below="@+id/frg_present_p_3_img_1"
        android:layout_marginBottom="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="40dp"
        android:layout_marginLeft="40dp"
        android:src="@drawable/present_p_3"
        />
   
</RelativeLayout>
